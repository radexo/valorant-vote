      {id:'diff10',text:'Ğ“Ğ¾Ğ»Ğ¾ÑĞ¾Ğ²Ğ°Ğ» Ğ·Ğ° 10 Ğ°Ğ³ĞµĞ½Ñ‚Ğ¾Ğ²',check:()=>agents.filter(a=>getVotes('up',a.name)>0).length>=10},
      {id:'both',text:'Ğ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ğ» Ğ¾Ğ±Ğ° Ñ‚Ğ¸Ğ¿Ğ°',check:()=>votes.usedBoth},
      {id:'all_up',text:'Ğ—Ğ° Ğ²ÑĞµÑ… Ğ°Ğ³ĞµĞ½Ñ‚Ğ¾Ğ²',check:()=>agents.every(a=>getVotes('up',a.name)>=1)},
      {id:'all_down',text:'ĞŸÑ€Ğ¾Ñ‚Ğ¸Ğ² Ğ²ÑĞµÑ… Ğ°Ğ³ĞµĞ½Ñ‚Ğ¾Ğ²',check:()=>agents.every(a=>getVotes('down',a.name)>=1)},
      {id:'twenty',text:'20 Ğ³Ğ¾Ğ»Ğ¾ÑĞ¾Ğ²',check:()=>votes.total>=20},
      {id:'secret',text:'Ğ¢Ğ°Ğ¹Ğ½Ñ‹Ğ¹ Ğ¼Ğ°ÑÑÑ‚Ñ€Ğ¾',secret:true,check:()=>votes.secret}
    ];

    function updateAchievementsSection(){
      const sec=document.getElementById('achievements-section');
      const cont=document.getElementById('achievements');cont.innerHTML='';let any=false;
      achievementsArr.forEach(a=>{if(localStorage.getItem(a.id)){any=true;const span=document.createElement('span');span.className='inline-flex items-center bg-yellow-400 text-black px-2 py-1 rounded space-x-1';span.innerHTML=`<span>${a.secret?'ğŸ”’':'âœ…'}</span><span>${a.text}</span>`;cont.appendChild(span);}});
      sec.classList.toggle('hidden',!any);
    }

    function checkAchievements(){achievementsArr.forEach(a=>{if(!localStorage.getItem(a.id)&&a.check()){localStorage.setItem(a.id,'1');if(a.secret)confetti({emojis:['ğŸ‰','âœ¨','ğŸ†'],emojiSize:40,particles:50});}});updateAchievementsSection();}

    function renderLeaderboard(){const bestEl=document.getElementById('best-list'),dumbEl=document.getElementById('dumb-list');bestEl.innerHTML='';dumbEl.innerHTML='';agents.slice().sort((a,b)=>getVotes('up',b.name)-getVotes('up',a.name)).slice(0,5).forEach((a,i)=>{const cls=i===0?'text-2xl':'text-base';bestEl.insertAdjacentHTML('beforeend',`<tr class='hover:bg-gray-50 dark:hover:bg-gray-700 ${cls}'><td class='p-2 font-semibold'>${['ğŸ†','ğŸ¥‡','ğŸ¥ˆ','ğŸ¥‰','ğŸ–ï¸'][i]||''} ${a.name}</td><td class='p-2 font-semibold text-green-600'>${getVotes('up',a.name)}</td></tr>`)});agents.slice().sort((a,b)=>getVotes('down',b.name)-getVotes('down',a.name)).slice(0,5).forEach((a,i)=>{const cls=i===0?'text-2xl':'text-base';dumbEl.insertAdjacentHTML('beforeend',`<tr class='hover:bg-gray-50 dark:hover:bg-gray-700 ${cls}'><td class='p-2 font-semibold'>${['ğŸ†','ğŸ¥‡','ğŸ¥ˆ','ğŸ¥‰','ğŸ–ï¸'][i]||''} ${a.name}</td><td class='p-2 font-semibold text-red-600'>${getVotes('down',a.name)}</td></tr>`)});}

    function renderAgents(){const grid=document.getElementById('agent-grid');grid.innerHTML='';agents.forEach(a=>{const card=document.createElement('div');card.className='bg-white dark:bg-gray-800 p-6 rounded-lg shadow flex flex-row justify-between';card.innerHTML=`<div class='w-3/4'><h3 class='text-lg font-semibold mb-2'>${a.name}</h3><p class='mb-1 text-green-600'><strong>Ğ¡Ğ¸Ğ»ÑŒĞ½Ğ°Ñ ÑÑ‚Ğ¾Ñ€Ğ¾Ğ½Ğ°:</strong> ${a.strong}</p><p class='mb-1 text-red-600'><strong>ĞŸĞ¾Ñ‡ĞµĞ¼Ñƒ Ğ½Ğµ Ğ±Ñ€Ğ°Ñ‚ÑŒ:</strong> ${a.weak}</p><p class='italic'><strong>Ğ¢Ğ¸Ğ¿Ğ¸Ñ‡Ğ½Ñ‹Ğ¹ ${a.name}:</strong> ${a.odd}</p></div><div class='w-1/4 flex flex-col items-center text-2xl'><button class='up text-green-500 mb-1' onclick="vote('${a.name}','up')">â–²</button><span id='up-${a.name}'>${getVotes('up',a.name)}</span><button class='down text-red-500 mt-1' onclick="vote('${a.name}','down')">â–¼</button><span id='down-${a.name}'>${getVotes('down',a.name)}</span></div>`;grid.appendChild(card);});}

    function vote(name,type){const curr=getVotes(type,name);if(curr<maxVotes){setVotes(type,name,curr+1);votes.total++;votes.usedBoth=votes.usedBoth||(getVotes('up',name)>0&&getVotes('down',name)>0);if(curr+1===maxVotes)votes.secret=true;document.getElementById(`${type}-${name}`).textContent=curr+1;renderLeaderboard();checkAchievements();}}

    document.addEventListener('DOMContentLoaded',()=>{renderLeaderboard();renderAgents();checkAchievements();});
  </script>
</body>
</html>
