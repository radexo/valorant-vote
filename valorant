    const maxVotes=3;
    function getVotes(k,n){return parseInt(localStorage.getItem(k+"_"+n))||0;}
    function setVotes(k,n,v){localStorage.setItem(k+"_"+n,v);}    
    const votes={total:0,usedBoth:false,secret:false};

    const achievementsArr=[
      {id:'first',text:'–ü–µ—Ä–≤—ã–π –≥–æ–ª–æ—Å',check:()=>votes.total>=1},
      {id:'five',text:'5 –≥–æ–ª–æ—Å–æ–≤',check:()=>votes.total>=5},
      {id:'ten',text:'10 –≥–æ–ª–æ—Å–æ–≤',check:()=>votes.total>=10},
      {id:'three_up',text:'3 –ø–ª—é—Å–∞ –æ–¥–Ω–æ–º—É',check:()=>agents.some(a=>getVotes('up',a.name)>=3)},
      {id:'three_down',text:'3 –º–∏–Ω—É—Å–∞ –æ–¥–Ω–æ–º—É',check:()=>agents.some(a=>getVotes('down',a.name)>=3)},
      {id:'diff10',text:'–ì–æ–ª–æ—Å–æ–≤–∞–ª –∑–∞ 10 –∞–≥–µ–Ω—Ç–æ–≤',check:()=>agents.filter(a=>getVotes('up',a.name)>0).length>=10},
      {id:'both',text:'–ò—Å–ø–æ–ª—å–∑–æ–≤–∞–ª –æ–±–∞ —Ç–∏–ø–∞',check:()=>votes.usedBoth},
      {id:'all_up',text:'–ó–∞ –≤—Å–µ—Ö –∞–≥–µ–Ω—Ç–æ–≤',check:()=>agents.every(a=>getVotes('up',a.name)>=1)},
      {id:'all_down',text:'–ü—Ä–æ—Ç–∏–≤ –≤—Å–µ—Ö –∞–≥–µ–Ω—Ç–æ–≤',check:()=>agents.every(a=>getVotes('down',a.name)>=1)},
      {id:'twenty',text:'20 –≥–æ–ª–æ—Å–æ–≤',check:()=>votes.total>=20},
      {id:'secret',text:'–¢–∞–π–Ω—ã–π –º–∞—ç—Å—Ç—Ä–æ',secret:true,check:()=>votes.secret}
    ];

    function updateAchievementsSection(){
      const sec=document.getElementById('achievements-section');
      const cont=document.getElementById('achievements');cont.innerHTML='';let any=false;
      achievementsArr.forEach(a=>{if(localStorage.getItem(a.id)){any=true;const span=document.createElement('span');span.className='inline-flex items-center bg-yellow-400 text-black px-2 py-1 rounded space-x-1';span.innerHTML=`<span>${a.secret?'üîí':'‚úÖ'}</span><span>${a.text}</span>`;cont.appendChild(span);}});
      sec.classList.toggle('hidden',!any);
    }

    function checkAchievements(){achievementsArr.forEach(a=>{if(!localStorage.getItem(a.id)&&a.check()){localStorage.setItem(a.id,'1');if(a.secret)confetti({emojis:['üéâ','‚ú®','üéÜ'],emojiSize:40,particles:50});}});updateAchievementsSection();}

    function renderLeaderboard(){const bestEl=document.getElementById('best-list'),dumbEl=document.getElementById('dumb-list');bestEl.innerHTML='';dumbEl.innerHTML='';agents.slice().sort((a,b)=>getVotes('up',b.name)-getVotes('up',a.name)).slice(0,5).forEach((a,i)=>{const cls=i===0?'text-2xl':'text-base';bestEl.insertAdjacentHTML('beforeend',`<tr class='hover:bg-gray-50 dark:hover:bg-gray-700 ${cls}'><td class='p-2 font-semibold'>${['üèÜ','ü•á','ü•à','ü•â','üéñÔ∏è'][i]||''} ${a.name}</td><td class='p-2 font-semibold text-green-600'>${getVotes('up',a.name)}</td></tr>`)});agents.slice().sort((a,b)=>getVotes('down',b.name)-getVotes('down',a.name)).slice(0,5).forEach((a,i)=>{const cls=i===0?'text-2xl':'text-base';dumbEl.insertAdjacentHTML('beforeend',`<tr class='hover:bg-gray-50 dark:hover:bg-gray-700 ${cls}'><td class='p-2 font-semibold'>${['üèÜ','ü•á','ü•à','ü•â','üéñÔ∏è'][i]||''} ${a.name}</td><td class='p-2 font-semibold text-red-600'>${getVotes('down',a.name)}</td></tr>`)});}

    function renderAgents(){const grid=document.getElementById('agent-grid');grid.innerHTML='';agents.forEach(a=>{const card=document.createElement('div');card.className='bg-white dark:bg-gray-800 p-6 rounded-lg shadow flex flex-row justify-between';card.innerHTML=`<div class='w-3/4'><h3 class='text-lg font-semibold mb-2'>${a.name}</h3><p class='mb-1 text-green-600'><strong>–°–∏–ª—å–Ω–∞—è —Å—Ç–æ—Ä–æ–Ω–∞:</strong> ${a.strong}</p><p class='mb-1 text-red-600'><strong>–ü–æ—á–µ–º—É –Ω–µ –±—Ä–∞—Ç—å:</strong> ${a.weak}</p><p class='italic'><strong>–¢–∏–ø–∏—á–Ω—ã–π ${a.name}:</strong> ${a.odd}</p></div><div class='w-1/4 flex flex-col items-center text-2xl'><button class='up text-green-500 mb-1' onclick="vote('${a.name}','up')">‚ñ≤</button><span id='up-${a.name}'>${getVotes('up',a.name)}</span><button class='down text-red-500 mt-1' onclick="vote('${a.name}','down')">‚ñº</button><span id='down-${a.name}'>${getVotes('down',a.name)}</span></div>`;grid.appendChild(card);});}

    function vote(name,type){const curr=getVotes(type,name);if(curr<maxVotes){setVotes(type,name,curr+1);votes.total++;votes.usedBoth=votes.usedBoth||(getVotes('up',name)>0&&getVotes('down',name)>0);if(curr+1===maxVotes)votes.secret=true;document.getElementById(`${type}-${name}`).textContent=curr+1;renderLeaderboard();checkAchievements();}}

    document.addEventListener('DOMContentLoaded',()=>{renderLeaderboard();renderAgents();checkAchievements();});
  </script>
</body>
</html>
